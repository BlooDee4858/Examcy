        <ul class="nav nav-tabs" id="myTab" role="tablist">
            <li class="nav-item" role="presentation">

              <button class="nav-link active" id="home-tab" data-bs-toggle="tab" data-bs-target="#allraiting" type="button" role="tab" aria-controls="home" aria-selected="true">Общий рейтинг</button>
            </li>
            <li class="nav-item" role="presentation">
              <button class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#testraiting" type="button" role="tab" aria-controls="profile" aria-selected="false">Тестовый вариант</button>
            </li>
            <li class="nav-item" role="presentation">
              <button class="nav-link" id="contact-tab" data-bs-toggle="tab" data-bs-target="#visitors" type="button" role="tab" aria-controls="contact" aria-selected="false">Посещаемость</button>
            </li>
        </ul>
        <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="allraiting" role="tabpanel" aria-labelledby="home-tab">
                    @if (@Model.allratings.Count != 0)
                    {
                        <div class="u-align-center u-container-style u-group u-group-1">
                            <div class="u-container-layout">
                                <span ><img src="~/img/11.png" width="100" height="100" alt=""></span>
                                @if (@Model.n_allraiting == 0)
                                {
                                    <h4 align="center">Поздравляю! Вы достигли вершины!</h4>
                                }
                                else
                                {
                                    <h4 align="center">@Model.n_allraiting осталось до вершины</h4>
                                }
                            </div>
                        </div>


                        <table class="u-table-entity">
                            <colgroup>
                                <col width="30.4%">
                                <col width="200.4%">
                                <col width="39.2%">
                            </colgroup>
                            <tbody class="u-table-alt-grey-5 u-table-body">
                                <tr style="height: 47px;">
                                    <td class="u-table-cell">Место</td>
                                    <td class="u-table-cell">Студент</td>
                                    <td class="u-table-cell">Опыт</td>
                                </tr>
                                @foreach (var l in Model.allratings)
                                {
                                    @if (@l.ExpAll > Model.first_allraiting)
                                    {
                                        <tr style="height: 51px; background-color:#adf5ab">
                                            <td class="u-table-cell">@l.numberAllRairing</td>
                                            <td class="u-table-cell">@l.LastName @l.FirstName</td>
                                            <td class="u-table-cell">@l.ExpAll</td> <!--общий рейтинг за курс-->
                                        </tr>
                                    }
                                    @if (@l.ExpAll >= Model.second_allraiting && @l.ExpAll <= Model.first_allraiting)
                                    {
                                        <tr style="height: 51px; background-color:#fff48c">
                                            <td class="u-table-cell">@l.numberAllRairing</td>
                                            <td class="u-table-cell">@l.LastName @l.FirstName</td>
                                            <td class="u-table-cell">@l.ExpAll</td> <!--общий рейтинг за курс-->
                                        </tr>
                                    }
                                    @if (@l.ExpAll != 0 && @l.ExpAll < Model.second_allraiting)
                                    {
                                        <tr style="height: 51px; background-color:#fcc572">
                                            <td class="u-table-cell">@l.numberAllRairing</td>
                                            <td class="u-table-cell">@l.LastName @l.FirstName</td>
                                            <td class="u-table-cell">@l.ExpAll</td> <!--общий рейтинг за курс-->
                                        </tr>
                                    }
                                    @if (@l.ExpAll == 0)
                                    {
                                        <tr style="height: 51px; background-color:#fc8b83">
                                            <td class="u-table-cell">@l.numberAllRairing</td>
                                            <td class="u-table-cell">@l.LastName @l.FirstName</td>
                                            <td class="u-table-cell">@l.ExpAll</td> <!--общий рейтинг за курс-->
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p style="text-align:center;">Для данного курса пока что нет рейтинга</p>   
                    }
                </div>
                    

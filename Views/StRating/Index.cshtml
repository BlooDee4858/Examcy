                                            <td class="u-table-cell">@l.numberVisitorsRairing</td>
                                            <td class="u-table-cell">@l.LastName @l.FirstName</td>
                                            <td class="u-table-cell">@l.Visitors</td> <!--общий рейтинг за курс-->
                                        </tr>
                                    }
                                    @if (@l.Visitors != 0 && @l.Visitors <= Model.second_visitors)
                                    {
                                        <tr style="height: 51px; background-color:#fcc572">
                                            <td class="u-table-cell">@l.numberVisitorsRairing</td>
                                            <td class="u-table-cell">@l.LastName @l.FirstName</td>
                                            <td class="u-table-cell">@l.Visitors</td> <!--общий рейтинг за курс-->
                                        </tr>
                                    }
                                    @if (@l.Visitors == 0)

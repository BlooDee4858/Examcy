@model RaitingCourseViewModel
<!DOCTYPE html>
<html style="font-size: 16px;">
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta charset="utf-8">
    
    <meta name="pagbottom content="np-template-header-footer-from-plugin">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <link rel="stylesheet" href="/css/nicepage.css" media="screen">
    <link rel="stylesheet" href="/css/student_rating.css" media="screen">
    <script class="u-script" type="text/javascript" src="/js/jquery.js" defer=""></script>
    <script class="u-script" type="text/javascript" src="/js/nicepage.js" defer=""></script>
    <link id="u-theme-google-font" rel="stylesheet" href="https://fonts.googleapis.com/css?family=Didact+Gothic:400">
    <link id="u-page-google-font" rel="stylesheet" href="https://fonts.googleapis.com/css?family=Didact+Gothic:400">

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

    @*<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">*@
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
 
    <meta name="theme-color" content="#9ea400">
    <meta property="og:type" content="website">
  </head>


<body style="padding-bottom: 0; margin-bottom: 0">
@Html.Partial("HeadStudent")
<article></article>
<article>
<h2 class="text-title">Рейтинг</h2>
    <table width="70%" style="margin-left: 15%"> <!--class="u-table-entity" -->
        <colgroup>
          <col width="40%">
          <col width="30%">
        </colgroup>
        <tbody class="u-table-alt-grey-5 u-table-body" align="center"> <!--class="u-table-alt-grey-5 u-table-body"-->
          <tr style="height: 47px;">
            @*<td class="u-table-cell">Место</td>
            <td class="u-table-cell">Студент</td>*@
          </tr>
          @if(Model != null)
          {
                @foreach (var l in Model.courselist){
                    <tr style="height: 51px;" >
                      <td align="left" style="padding-left=5%">@l.Title</td> <!--class="u-table-cell"-->
                      <td align="right" style="padding-right=5%">
                          <a href="~/StRating/Index/@l.Id"> Посмотреть рейтинг</a></td>
                    </tr>
                }
          }
          else
          {
              <tr>
            <td class="u-table-cell"> Запишитесь на курс </td>  
            </tr>
          }
            </tbody>
    </table>
</article>
<article></article>
  <footer id="pageFooter" class="footer-all u-clearfix">
    <p class="footer-text u-text-variant"> © 2022 Examcy | Образовательная платформа</p>
  </footer>
</body>
</html>
